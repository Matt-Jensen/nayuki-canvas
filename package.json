{
  "name": "nayuki-canvas",
  "version": "0.0.1",
  "description": "A floating node HTML5 canvas",
  "main": "dist/nayuki-canvas.js",
  "scripts": {
    "lint": "eslint src scripts tests",
    "pretest": "npm run -s lint && node sripts/build.js dev && node scripts/build-tests.js",
    "posttest": "node scripts/clean-tests.js",
    "build": "node scripts/build.js",
    "test": "tape tests/**/*.js",
    "watch": "watch 'clear && npm run -s test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-jensen/nayuki-canvas.git"
  },
  "keywords": [
    "canvas",
    "floating",
    "nodes",
    "2d"
  ],
  "author": "Matt-Jensen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matt-jensen/nayuki-canvas/issues"
  },
  "homepage": "https://github.com/matt-jensen/nayuki-canvas",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "del": "^2.2.0",
    "eslint": "^2.10.2",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-uglify": "^0.3.1",
    "tap-colorize": "^1.2.0",
    "tape": "^4.5.1",
    "watch": "^0.18.0"
  }
}
