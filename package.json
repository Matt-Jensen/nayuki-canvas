{
  "name": "nayuki-canvas",
  "version": "0.0.1",
  "description": "A floating node HTML5 canvas",
  "main": "dist/nayuki-canvas.js",
  "scripts": {
    "lint": "./node_modules/.bin/gulp lint",
    "pretest": "npm run -s lint && node scripts/build-tests.js",
    "posttest": "node scripts/clean-tests.js",
    "build": "./node_modules/.bin/gulp",
    "test": "tape tests/**/*.js",
    "watch": "watch 'clear && npm run -s test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-jensen/nayuki-canvas.git"
  },
  "keywords": [
    "canvas",
    "floating",
    "nodes",
    "2d"
  ],
  "author": "Matt-Jensen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matt-jensen/nayuki-canvas/issues"
  },
  "homepage": "https://github.com/matt-jensen/nayuki-canvas",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-jscs": "^3.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^1.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tape": "0.0.9",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "tap-colorize": "^1.2.0",
    "tape": "^4.5.1"
  }
}
